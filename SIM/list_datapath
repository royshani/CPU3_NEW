onerror {resume}
add list -width 18 /tb_datapath/clk
add list /tb_datapath/TBactive
add list -hex /tb_datapath/mapDatapath/BusSignal
add list /tb_datapath/Control_BusAccess
add list /tb_datapath/Control_IRin
add list /tb_datapath/Control_RF_addr
add list /tb_datapath/Control_RF_WregEn
add list /tb_datapath/Control_RF_rst
add list /tb_datapath/Control_Ain
add list /tb_datapath/Control_Cin
add list /tb_datapath/Control_ALU_op
add list /tb_datapath/Control_Mem_in
add list /tb_datapath/Control_Data_MemEn
add list /tb_datapath/Control_Pcin
add list /tb_datapath/Control_PCsel
add list /tb_datapath/Out_Op_code
configure list -usestrobe 0
configure list -strobestart {0 ps} -strobeperiod {0 ps}
configure list -usesignaltrigger 1
configure list -delta none
configure list -signalnamewidth 1
configure list -datasetprefix 0
configure list -namelimit 5
