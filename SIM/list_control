onerror {resume}
add list -width 17 /tb_control/clk
add list -hex /tb_control/opcode
add list /tb_control/PCsel
add list /tb_control/Pcin
add list /tb_control/IRin
add list /tb_control/RF_addr
add list /tb_control/RF_WregEn
add list /tb_control/RF_out
add list /tb_control/Data_mem_out
add list /tb_control/Cout
add list /tb_control/Imm2_in
add list /tb_control/Imm1_in
add list /tb_control/Ain
add list /tb_control/Cin
add list /tb_control/ALU_op
add list /tb_control/Mem_in
add list /tb_control/Data_MemEn
add list /tb_control/RF_rst
add list /tb_control/done
configure list -usestrobe 0
configure list -strobestart {0 ps} -strobeperiod {0 ps}
configure list -usesignaltrigger 1
configure list -delta none
configure list -signalnamewidth 1
configure list -datasetprefix 0
configure list -namelimit 5
